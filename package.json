    "brfs": "^1.4.3",
    "choo": "^6.0.0",
    "enoki": "^1.0.2",
    "nano-markdown": "^1.2.0",
    "nanocomponent": "^6.0.0-1",
    "object-keys": "^1.0.11",
    "object-values": "^1.0.0",
    "query-string": "^5.0.0",
    "require-globify": "^1.4.1",
    "simplemde": "^1.11.2",
    "tags-input": "^1.1.1",
    "xhr": "^2.4.0",
    "xtend": "^4.0.1",
    "yo-yoify": "^3.7.3"
  },
